/*
С некоторого момента прошло 234 дня.
Сколько полных недель прошло за этот период?
Сколько месяцев?
*/
import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a=234;
      int b=7;
      int c=30;
      int result1=a/b;
      int result2=a/c;
      System.out.println(result1); //33
      System.out.println(result2); //7
  }
}


/*
Дано двузначное число 39.
Найти:
a.число десятков внем;
b.число единиц в нем;
c.сумму его цифр;
d.произведение его цифр.
*/
import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a=39;
      int result1=a/10;
      int result2=a/1;
      int result3=3+9;
      int result4=3*9;
      System.out.println(result1); //3
      System.out.println(result2); //39
      System.out.println(result3); //12
      System.out.println(result4); //27
  }
}
