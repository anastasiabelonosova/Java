/*
Дано двузначное число 39.
Найти:
a.число десятков внем;
b.число единиц в нем;
c.сумму его цифр;
d.произведение его цифр.
*/
import java.util.*;

public class Main {
    public static void main(String[] args) {
      int a=39;
      int result1=a/10;
      int result2=a/1;
      int result3=3+9;
      int result4=3*9;
      System.out.println(result1); //3
      System.out.println(result2); //39
      System.out.println(result3); //12
      System.out.println(result4); //27
  }
}
