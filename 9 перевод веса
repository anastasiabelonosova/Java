/* Таблица соответствия между весом в футах и килограммах 
   для значений 1-10 футов
*/

import java.util.*;

public class Main {
    public static void main(String[] args) {
      double a=453;
      for (double i=1;i<=10;i++){
      System.out.printf("%.0f фунт = %.3f кг\n", i,i*a/1000);
      }
  }
}
