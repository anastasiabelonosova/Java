/* Найти сумму всех чисел от a до b
(значения a и b вводятся с клавиатуры,b>=a)
*/

import java.util.*;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
      
      Scanner sc= new Scanner(System.in);
      
      int a=sc.nextInt();
      int b=sc.nextInt();
      
      int sum=0;
      
      for (int i=a;i<=b;i++){
        sum=sum+i;
      }
     
      System.out.printf("sum="+sum);
  }  
}
